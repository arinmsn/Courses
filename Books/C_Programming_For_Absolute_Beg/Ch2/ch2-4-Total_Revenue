#include <stdio.h>

int main()
{
    float fPrice = 0;
    int iQty = 0;
    float fTotalRevenue;
    
    printf("\nEnter a quantity: ");
    scanf("%i", &iQty);
    printf("Enter the price: ");
    scanf("%f", &fPrice);
    printf("Total revenue: %.2f", fTotalRevenue = iQty * fPrice);
    return 0;
}
